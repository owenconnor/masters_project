<div class="row">
    <div class="span2">
        <h3>Searches</h3>
        <ul class="nav nav-list">
            <li class="nav-header">Your Active Searches</li>
            <li class="active"><a href="#">Home</a></li>
            <li><a href="#">Library</a></li>
            <li class="nav-header">Active Newsroom Searches</li>
            <li><a href="#">Home</a></li>
            <li><a href="#">Library</a></li>
            <li class="nav-header">Recent Searches</li>
            <li><a href="#">Home</a></li>
            <li><a href="#">Library</a></li>

        </ul>
    </div><!-- EO span3 -->
    <div class="span12">
        <h1><%= @search.search_name %></h1>
        <h5>Created:<%= @search.created_at %></h5>
         <div class="span9">
            <div id="filter-button">
                <div class="btn-group">
                    <button class="btn">Fliter By</button>
                    <button class="btn dropdown-toggle" data-toggle="dropdown">
                        <span class="caret"></span>
                    </button>
                    <ul class="dropdown-menu">
                        <!-- dropdown menu links -->
                        <li> <button class="btn btn-mini">Ranking</button></li>
                        <li>Uploader</li>
                        <li>Published</li>
                        <li>Location</li>
                    </ul>
                </div><!-- EO btn-group -->
            </div><!-- EO filter-button -->
        </div>

        
       
            <ul class="thumbnails">
                <% @yt_search_results.each do |yt_search_result| %>
                <li class="span4" id="video_thumb_box">
                    <a href="#" class="thumbnail">
                    <%= image_tag("#{yt_search_result.thumbnails}", :height => '240', :width => '320') %>
                    </a>
                    <h6><%= yt_search_result.title %></h6>
                    <h6>Published on <%= yt_search_result.published %> by <%=link_to "#{yt_search_result.author_name}", yt_search_result.author_url %> </h6>
                    Filed in<%= yt_search_result.category %>
                    <h6>Duration: <%= yt_search_result.duration %></h6>
                    <h6>Viewcount: <%= yt_search_result.viewcount %></h6>
                    <h6>Location: <%= yt_search_result.geo %></h6>
                    <%= link_to "View", yt_search_result_path(yt_search_result.id), :class => 'btn btn-mini'%>
                </li>
                <% end %>
            </ul>
        

            
    </div><!-- EO span9 -->
</div> <!-- EO row -->
