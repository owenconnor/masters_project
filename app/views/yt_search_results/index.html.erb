<div class="row">
    <div class="span3"><h3>Active Searches</h3>
        <ul class="nav nav-list">
            <li class="nav-header">List header <span class="badge badge-warning">4</span></li>
            <li class="active"><a href="#">Home</a></li>
            <li><a href="#">Library</a></li>
        </ul>
    </div><!-- EO span3 -->
    <div class="span9">
        <h1>Listing Search Results</h1>
        
       
            <ul class="thumbnails">
                <% @yt_search_results.each do |yt_search_result| %>
                <li class="span3">
                    <a href="#" class="thumbnail">
                    <%= image_tag("#{yt_search_result.thumbnails}", :height => '180', :width => '240') %>
                    </a>
                    <h6>Published Date: <%= yt_search_result.published %></h6>
                    <h6>Duration: <%= yt_search_result.duration %></h6>
                    <h6>Viewcount: <%= yt_search_result.viewcount %></h6>
                    <h6>Location: <%= yt_search_result.geo %></h6>
                    <h6>Author: <%=link_to "#{yt_search_result.author_name}", yt_search_result.author_url %></h6>
                    <h6>Category: <%= yt_search_result.category %></h6>
                </li>
                <% end %>
            </ul>
        

            
    </div><!-- EO span9 -->
</div> <!-- EO row -->
