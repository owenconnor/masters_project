<%= form_for @yt_search_result, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :published, :class => 'control-label' %>
    <div class="controls">
      <%= f.date_select :published, :class => 'date_select' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :updated, :class => 'control-label' %>
    <div class="controls">
      <%= f.date_select :updated, :class => 'date_select' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :title, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :title, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :description, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :description, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :thumbnails, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :thumbnails, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :search_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :search_id, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :embed_url, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :embed_url, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :author_name, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :author_name, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :author_url, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :author_url, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :category, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :category, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :keywords, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :keywords, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :player_url, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :player_url, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :duration, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :duration, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :viewcount, :class => 'control-label' %>
    <div class="controls">
      <%= f.number_field :viewcount, :class => 'number_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :geo, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :geo, :class => 'text_field' %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label :video_id, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :video_id, :class => 'text_field' %>
    </div>
  </div>

  <div class="form-actions">
    <%= f.submit nil, :class => 'btn btn-primary' %>
    <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                yt_search_results_path, :class => 'btn' %>
  </div>
<% end %>
