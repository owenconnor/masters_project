<%- model_class = Search -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
 
  <dt><strong><%= model_class.human_attribute_name(:location) %>:</strong></dt>
  <dd><%= @search.location %></dd>
  <dt><strong><%= model_class.human_attribute_name(:latitude) %>:</strong></dt>
  <dd><%= @search.latitude %></dd>
  <dt><strong><%= model_class.human_attribute_name(:longitude) %>:</strong></dt>
  <dd><%= @search.longitude %></dd>
  <dt><strong><%= model_class.human_attribute_name(:vicinity) %>:</strong></dt>
  <dd><%= @search.vicinity %></dd>
  <dt><strong><%= model_class.human_attribute_name(:search_name) %>:</strong></dt>
  <dd><%= @search.search_name %></dd>
  <dt><strong><%= model_class.human_attribute_name(:search_concept_id) %>:</strong></dt>
  <dd><%= @search.search_concept_id %></dd>
    <dt><strong><%= model_class.human_attribute_name(:second_language) %>:</strong></dt>
  <dd><%= @search.second_language %></dd>
    <dt><strong><%= model_class.human_attribute_name(:context_terms) %>:</strong></dt>
  <dd><%= @search.context_terms %></dd>
  



</dl>

<div class="form-actions">
  <%= link_to t('.search'),
              youtube_search_path(:search_id => @search.id), :class => 'btn'  %>
  <%= link_to t('.back', :default => t("helpers.links.back")),
              searches_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_search_path(@search), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              search_path(@search),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
